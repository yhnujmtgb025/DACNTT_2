                        <path id="t4"
                            d="M34.6 6.1c5.7 0 10.4 5.2 10.4 11.5 0 6.8-5.9 11-11.5 16S25 41.3 24 41.9c-1.1-.7-4.7-4-9.5-8.3-5.7-5-11.5-9.2-11.5-16C3 11.3 7.7 6.1 13.4 6.1c4.2 0 6.5 2 8.1 4.3 1.9 2.6 2.2 3.9 2.5 3.9.3 0 .6-1.3 2.5-3.9 1.6-2.3 3.9-4.3 8.1-4.3m0-3c-4.5 0-7.9 1.8-10.6 5.6-2.7-3.7-6.1-5.5-10.6-5.5C6 3.1 0 9.6 0 17.6c0 7.3 5.4 12 10.6 16.5.6.5 1.3 1.1 1.9 1.7l2.3 2c4.4 3.9 6.6 5.9 7.6 6.5.5.3 1.1.5 1.6.5.6 0 1.1-.2 1.6-.5 1-.6 2.8-2.2 7.8-6.8l2-1.8c.7-.6 1.3-1.2 2-1.7C42.7 29.6 48 25 48 17.6c0-8-6-14.5-13.4-14.5z">
                        </path>
                    </svg>
                </a>
                <div class="molHeart">
                    <!-- modal icon profile -->
                    <div class="coverHeart" id="modalHeart">
                        <!-- Modal content -->
                        <div class="conHeart">
                            <img src="/img/triangle.png" width="25" height="20" id="rhombus-heart" alt="">
                            <ul class="list-group listPro1" id="list-noti" >
                                <li >
                                    <a href="#"> </a>
                                </li>
                            </ul>
                        </div>

                    </div>

                </div>

                <!-- profile -->
                <div class="mol" id="modProfile">
                    <%if(userCurrent!=""){%>
                        <img src="<%=imgCurrent%>" id="iconProfile" width="46" height="24" alt=""
                            style="border-radius:50%;padding: 0 10px;margin-left: 5px; ">
                    <%}else{%>
                        <img src="<%=image%>" id="iconProfile" width="46" height="24" alt=""
                            style="border-radius:50%;padding: 0 10px;margin-left: 5px; ">
                    <%}%>
                        <!-- modal icon profile -->
                        <div class="coverPro" id="modalProfile">
